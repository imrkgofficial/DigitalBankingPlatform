{
    "compilerOptions": {
      "target": "es2015",
      "module": "es2020",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "experimentalDecorators": true,
      "skipLibCheck": true,
      "strict": true,
      "noImplicitAny": false,
      "baseUrl": "./",
      "paths": {
        "@angular/*": [
          "node_modules/@angular/*"
        ]
      }
    }
  }
  